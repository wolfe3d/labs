function _setPerspectiveState() {
  set perspectiveModel 11;
  set scaleAngstromsPerInch 0.0;
  set perspectiveDepth true;
  set visualRange 5.0;
  set cameraDepth 3.0;
  boundbox corners {-12.911999 9.710999 -41.449997} {32.279 55.155 8.489002} # volume = 102557.71;
  center {9.6835 32.433 -16.4805};
   moveto -1.0 { 640 500 -583 135.13} 49.72 0.0 0.0 {9.6835 32.433 -16.4805} 29.181444 {0 0 0} 0 0 0 3.0 0.0 0.0;
  save orientation "";
   moveto -1.0 {0 0 1 0} 100.0 0.0 0.0 {9.6835 32.433 -16.4805} 29.181444 {0 0 0} 0 0 0 3.0 0.0 0.0;
  save orientation "default";
  moveto 0.0 { -695 553 -459 106.33} 100.0 0.0 0.0 {9.6835 32.433 -16.4805} 29.181444 {0 0 0} 0 0 0 3.0 0.0 0.0;;
  slab 100;depth 0;
  set slabRange 0.0;
  set spinX 0; set spinY 30; set spinZ 0; set spinFps 30;  set navX 0; set navY 0; set navZ 0; set navFps 10;
}

